<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <br>
   <%= f.label "Genre: " %>
   <%= f.collection_select :genre_name, Genre.all, :name, :name, {}, {id: "song_genre_id"}%>
  <br>
  <%= f.text_field :note_contents, multiple: true, id: "song_notes_1" %>
  <%= f.text_field :note_contents, multiple: true, id: "song_notes_2" %>
  <%= f.text_field :note_contents,  multiple: true, id: "song_notes_3" %>

  <%= f.submit %>
<% end %>
